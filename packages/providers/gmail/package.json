{
  "name": "@email-sdk/provider-gmail",
  "description": "Gmail API provider plugin for Email SDK",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
